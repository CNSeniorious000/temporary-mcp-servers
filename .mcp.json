{
  "mcpServers": {
    "discord": {
      "command": "uv",
      "args": [
        "run",
        "mcp-hmr",
        "discord-mcp.py:app"
      ]
    },
    "py": {
      "command": "uv",
      "args": [
        "run",
        "mcp-hmr",
        "ipython-mcp.py:mcp"
      ]
    },
    "web": {
      "command": "uv",
      "args": [
        "run",
        "webview-mcp.py"
      ]
    },
    "xiaoyuzhoufm": {
      "command": "uv",
      "args": [
        "run",
        "mcp-hmr",
        "xiaoyuzhoufm-mcp.py:app"
      ]
    }
  }
}
